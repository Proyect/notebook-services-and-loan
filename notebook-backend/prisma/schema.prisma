generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Notebook {
  id          Int      @id @default(autoincrement())
  marca       String
  modelo      String
  numeroSerie String   @unique
  idHardware  String
  estado      String
  observacion String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
